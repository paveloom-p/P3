ext=f95
comp=gfortran
files=$(wildcard *.f95)
obj=$(patsubst %.$(ext), %.o, $(files))
name=Run

$(name): TypeCoord.f95 procedures.f95 main.f95
	$(comp) TypeCoord.f95 procedures.f95 main.f95 -o $@
Go:$(name)
	./$(name) < input > result
clean:
	rm -f $(obj)
exterminate:
	rm -f $(obj) $(name) $(wildcard *.o) $(wildcard *.mod)
